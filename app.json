{
  "expo": {
    "name": "Everlast",
    "slug": "Everlast",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#785846"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uhuyy.Everlast"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.uhuyy.Everlast"
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    },
    "expo": {
      "splash": {
        "image": "./assets/icons/Logo.png",
        "backgroundColor": "785846"
      }
    },
    "owner": "tubes-mobile-expo",
    "extra": {
      "eas": {
        "projectId": "2ee0eefd-eb92-427a-8fc9-a660b2e40095"
      }
    },
    "plugins": [
      [
        "expo-font",
        {
          "poppinsblack": "./assets/fonts/poppinsblack.ttf",
          "poppinsblackitalic": "./assets/fonts/poppinsblackitalic.ttf",
          "poppinsbold": "./assets/fonts/poppinsbold.ttf",
          "poppinsbolditalic": "./assets/fonts/poppinsbolditalic.ttf",
          "poppinsextrabold": "./assets/fonts/poppinsextrabold.ttf",
          "poppinsextrabolditalic": "./assets/fonts/poppinsextrabolditalic.ttf",
          "poppinsextralight": "./assets/fonts/poppinsextralight.ttf",
          "poppinsextralightitalic": "./assets/fonts/poppinsextralightitalic.ttf",
          "poppinsitalic": "./assets/fonts/poppinsitalic.ttf",
          "poppinslight": "./assets/fonts/poppinslight.ttf",
          "poppinslightitalic": "./assets/fonts/poppinslightitalic.ttf",
          "poppinsmedium": "./assets/fonts/poppinsmedium.ttf",
          "poppinsmediumitalic": "./assets/fonts/poppinsmediumitalic.ttf",
          "poppinsregular": "./assets/fonts/poppinsregular.ttf",
          "poppinssemibold": "./assets/fonts/poppinssemibold.ttf",
          "poppinssemibolditalic": "./assets/fonts/poppinssemibolditalic.ttf",
          "poppinsthin": "./assets/fonts/poppinsthin.ttf",
          "poppinsthinitalic": "./assets/fonts/poppinsthinitalic.ttf"
        }
      ]
    ]
  }
}
